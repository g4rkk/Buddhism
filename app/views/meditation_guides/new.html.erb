<h1>新しい瞑想ガイドを作成</h1>
<%= form_with(model: @meditation_guide, local: true) do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.label :description %>
    <%= form.text_area :description %>
  </div>
  <div>
    <%= form.label :audio %>
    <%= form.file_field :audio %>
  </div>
  <%= form.submit '作成' %>
<% end %>